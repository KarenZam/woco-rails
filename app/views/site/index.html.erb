	<section id="contact">
		<div class="container">
			<div class="row">
				<h1>Contact</h1>
			</div>
			<div class="row">
                <div class="join-us"><b>Join us on march 27th, 7:00-9:00pm</b></div> 
				<div class="col-sm-6 col-lg-6 fadeinleftbig">
					<h3 style="color:black !important">

						<strong>Come, share, meet some great coders!</strong><br>
                        Paperclip <br>
						3/F, Nam Wo Hong Building,<br>
						148 Wing Lok Street,<br>
						Sheung Wan, Hong Kong<br>
					
					</h3>
					<ul class="social-icons">
						<li>
							
							<a href="https://www.facebook.com/groups/womenwhocodehk/">
								<i class="typcn typcn-social-facebook-circular"></i>
							</a>
							
						</li>

					</ul>
				</div>
			
			<div class="col-sm-6 col-lg-6 fadeinrightbig">
			<div class="contact_form pad30">  

        <div class="col-md-12 col-lg-12">
          <form accept-charset="UTF-8" action="<%= contact_url %>" id="ajax-contact-form" method="post">
            <fieldset>
              <legend>Want to know more? We'd love to hear from you!</legend>
              <div class="contact error-message"></div>
              <div class="input-group">
                <label for="contact-email" class="sr-only">Email</label>
                <input type="email" id="contact-email" name="email" placeholder="Email..." class="form-control" required>
              </div>
              <div class="input-group">
                <label for="contact-subject" class="sr-only">Subject</label>
                <input type="text" id="contact-subject" name="subject" placeholder="Subject..." class="form-control" required>
              </div>
              <div class="input-group">
                <label for="contact-body" class="sr-only">Message</label>
                <textarea id="contact-body" name="body" placeholder="Message..." class="form-control"></textarea>
